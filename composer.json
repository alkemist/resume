{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/orm": "^2.12",
    "easycorp/easyadmin-bundle": "^4.20",
    "gumlet/php-image-resize": "^2.0",
    "karser/karser-recaptcha3-bundle": "^0.1.22",
    "konekt/pdf-invoice": "^1.12",
    "nelmio/cors-bundle": "^2.3",
    "nyholm/psr7": "^1.8",
    "php-ds/php-ds": "^1.4",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.6",
    "smalot/pdfparser": "^2.2",
    "symfony/asset": "7.2.*",
    "symfony/asset-mapper": "7.2.*",
    "symfony/console": "*",
    "symfony/doctrine-messenger": "7.2.*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "7.2.*",
    "symfony/flex": "^2",
    "symfony/form": "7.2.*",
    "symfony/framework-bundle": "*",
    "symfony/google-mailer": "7.2.*",
    "symfony/http-client": "7.2.*",
    "symfony/intl": "7.2.*",
    "symfony/mailer": "7.2.*",
    "symfony/mailjet-mailer": "7.2.*",
    "symfony/mime": "7.2.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "7.2.*",
    "symfony/process": "7.2.*",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/proxy-manager-bridge": "6.3.*",
    "symfony/runtime": "*",
    "symfony/security-bundle": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/stimulus-bundle": "^2.10",
    "symfony/string": "7.2.*",
    "symfony/translation": "7.2.*",
    "symfony/twig-bridge": "7.2.*",
    "symfony/twig-bundle": "7.2.*",
    "symfony/uid": "7.2.*",
    "symfony/ux-chartjs": "^2.10",
    "symfony/ux-twig-component": "^2.23",
    "symfony/validator": "7.2.*",
    "symfony/web-link": "7.2.*",
    "symfony/webpack-encore-bundle": "^2.1",
    "symfony/yaml": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/intl-extra": "^3.8",
    "twig/twig": "^3.20",
    "vich/uploader-bundle": "*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "deploy": "git pull; composer install; php bin/console do:sc:up --force --complete; yarn encore production;",
    "server": "symfony server:start",
    "rector": "vendor/bin/rector process",
    "crud": "bin/console make:admin:crud",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.13.7",
    "symfony/browser-kit": "7.2.*",
    "symfony/css-selector": "7.2.*",
    "symfony/debug-bundle": "7.2.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/stopwatch": "7.2.*",
    "symfony/var-dumper": "7.2.*",
    "symfony/web-profiler-bundle": "7.2.*"
  }
}
